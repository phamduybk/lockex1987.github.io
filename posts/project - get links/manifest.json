{
	"manifest_version": 2,
	"name": "Get Links",
	"version": "1.0.0",
	"description": "Get links to download",
	"author": "lockex1987",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.truyentranhvang.net/*"
			],
			"js": ["js/truyentranhvang.js"]
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		},
		"default_title" : "Get Links",
		"default_popup" : "popup.html"
	},
	"permissions" : [
	    "tabs", "notifications"
	]
}
