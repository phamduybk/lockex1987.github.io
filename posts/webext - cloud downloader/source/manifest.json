{
	"manifest_version": 2,
	"name": "Cloud Downloader",
	"version": "1.1.0",
	"description": "Download from userscloud, zippyshare automatically.",
	"icons": {
		"48": "icon/frog-icon-48.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*.userscloud.com/*"],
			"js": ["userscloud-downloader.js"]
		},
		{
			"matches": ["*://*.zippyshare.com/*"],
			"js": ["zippyshare-downloader.js"]
		}
	]
}
